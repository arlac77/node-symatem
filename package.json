{
  "name": "symatem",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "exports": {
    ".": "./src/api.mjs"
  },
  "description": "Symatem API",
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    },
    {
      "name": "Alexander MeiÃŸner"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m tests/*.mjs && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme ./src/api.mjs --section=API",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint ./src/api.mjs"
  },
  "dependencies": {
    "msgpack": "^1.0.2"
  },
  "devDependencies": {
    "ava": "^3.14.0",
    "c8": "^7.3.5",
    "documentation": "github:arlac77/documentation",
    "semantic-release": "^17.3.0"
  },
  "engines": {
    "node": ">=14.15.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/node-symatem.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/node-symatem/issues"
  },
  "homepage": "https://github.com/arlac77/node-symatem#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-esm-only"
    ]
  }
}
