{
  "name": "symatem",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/api.js",
  "module": "src/api.js",
  "description": "Symatem API",
  "keywords": [],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    },
    {
      "name": "Alexander MeiÃŸner"
    }
  ],
  "license": "MIT",
  "scripts": {
    "cover": "c8 --temp-directory build/tmp ava && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/api.js --section=API",
    "lint": "documentation lint src/api.js",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "pretest": "rollup -c tests/rollup.config.js",
    "test": "ava"
  },
  "dependencies": {
    "msgpack": "^1.0.2"
  },
  "devDependencies": {
    "ava": "^1.4.1",
    "builtin-modules": "^3.1.0",
    "c8": "^4.1.5",
    "codecov": "^3.3.0",
    "documentation": "^11.0.0",
    "esm": "^3.2.22",
    "markdown-doctest": "^0.9.1",
    "rollup": "^1.11.3",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-executable": "^1.4.5",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^4.2.4",
    "semantic-release": "^15.13.13"
  },
  "engines": {
    "node": ">=10.15.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/node-symatem.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/node-symatem/issues"
  },
  "homepage": "https://github.com/arlac77/node-symatem#readme",
  "ava": {
    "files": [
      "tests/*-test.js",
      "tests/*-test.mjs"
    ],
    "require": [
      "esm"
    ],
    "babel": false,
    "compileEnhancements": false,
    "extensions": [
      "js",
      "mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
