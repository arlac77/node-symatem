{
  "name": "symatem",
  "version": "0.0.0-semantic-release",
  "license": "MIT",
  "description": "Symatem API",
  "module": "src/api.js",
  "main": "dist/api.js",
  "scripts": {
    "cover": "nyc --temp-directory build/nyc ava",
    "test": "ava",
    "semantic-release": "semantic-release",
    "docs": "documentation readme src/api.js --section=API",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "precover": "rollup -c tests/rollup.config.js",
    "prepare": "rollup -c",
    "lint": "documentation lint src/api.js"
  },
  "bugs": {
    "url": "https://github.com/arlac77/node-symatem/issues"
  },
  "homepage": "https://github.com/arlac77/node-symatem#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arlac77/node-symatem.git"
  },
  "dependencies": {
    "msgpack": "^1.0.2"
  },
  "devDependencies": {
    "codecov": "^3.0.0",
    "markdown-doctest": "^0.9.1",
    "rollup": "^0.53.0",
    "semantic-release": "^11.1.0",
    "nyc": "^11.4.1",
    "ava": "^0.24.0",
    "rollup-plugin-multi-entry": "^2.0.2",
    "xo": "^0.19.0",
    "documentation": "^5.3.5"
  },
  "engines": {
    "node": ">=8.9.3"
  },
  "keywords": [],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    },
    {
      "name": "Alexander Mei√üner"
    }
  ],
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./coverage"
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  },
  "xo": {
    "space": true
  },
  "ava": {
    "files": [
      "build/*-test.js"
    ],
    "presets": [
      "env"
    ],
    "require": [
      "babel-register"
    ]
  }
}
